{
  "name": "Embedded Test Broker",
  "modelVersion": "6.1",
  "authenticationproviders": [
    {
      "name": "password",
      "type": "Plain",
      "secureOnlyMechanisms": [],
      "users": [
        {
          "name": "${qpid.username}",
          "password": "${qpid.password}",
          "type": "managed"
        }
      ]
    }
  ],
  "ports": [
    {
      "name": "AMQP",
      "port": "${qpid.port}",
      "authenticationProvider": "password",
      "protocols": [
        "AMQP_0_9_1"
      ],
      "transports": [
        "TCP"
      ],
      "virtualhostaliases": [
        {
          "name": "${qpid.vhost}",
          "type": "nameAlias"
        }
      ]
    }
  ],
  "virtualhostnodes": [
    {
      "name": "${qpid.vhost}",
      "type": "Memory",
      "virtualHostInitialConfiguration": "{ \"type\": \"Memory\" }"
    }
  ]
}